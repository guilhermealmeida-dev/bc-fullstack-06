{
  "get": {
    "summary": "Buscar dados do usuário",
    "description": "Endpoint para buscar dados do usuário logado.",
    "tags": ["User"],
    "security": [
      {
        "bearerAuth": []
      }
    ],
    "responses": {
      "200": {
        "description": "OK",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../../components/schemas.json#/UserResponseSchema"
            }
          }
        }
      },
      "401": {
        "description": "Unauthorized",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "error": {
                  "type": "string",
                  "example": "Autenticação necessária."
                }
              }
            }
          }
        }
      },
      "403": {
        "description": "Forbidden",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "error": {
                  "type": "string",
                  "example": "Esta conta foi desativada e não pode ser utilizada."
                }
              }
            }
          }
        }
      },
      "500": {
        "$ref": "../../components/responses.json#/InternalServerError"
      }
    }
  }
}
